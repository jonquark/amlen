kind: Service
apiVersion: v1
metadata:
  name: "{{ _amlen_name }}"
  namespace: "{{ _amlen_namespace }}"
  labels:
    app: "{{ _amlen_name }}"
spec:
  selector:
    app: "{{ _amlen_name }}"
  ports:
  - name: amlen
    port: 9089
    protocol: TCP
    targetPort: 9089
  sessionAffinity: None
  type: ClusterIP
